meta {
  name: Get tags
  type: http
  seq: 3
}

get {
  url: {{host}}/api/tags
  body: none
  auth: none
}

assert {
  res.status: eq 200
}

script:post-response {
  expect(Array.isArray(res.body.tags)).to.eql(true);
  expect(res.body.tags.length).to.be.at.least(1);
  expect(res.body.tags).to.include("h_" + bru.getVar("uid"));
  expect(res.body.tags).to.include("t_" + bru.getVar("uid"));
  expect(typeof res.body.tags[0]).to.eql("string");
  expect(typeof res.body.tags[1]).to.eql("string");
}
