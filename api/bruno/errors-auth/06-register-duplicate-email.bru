meta {
  name: Register duplicate email
  type: http
  seq: 6
}

post {
  url: {{host}}/api/users
  body: json
  auth: none
}

body:json {
  {
    "user": {
      "username": "ea_dup2_{{uid}}",
      "email": "ea_dup_{{uid}}@test.com",
      "password": "password123"
    }
  }
}

assert {
  res.status: eq 409
}

script:post-response {
  expect(res.body.errors.email[0]).to.eql("has already been taken");
}
