meta {
  name: Verify unfavorite persists
  type: http
  seq: 8
}

get {
  url: {{host}}/api/articles/{{slug}}
  body: none
  auth: none
}

headers {
  Authorization: Token {{token}}
}

assert {
  res.status: eq 200
}

script:post-response {
  expect(res.body.article.favorited).to.eql(false);
  expect(res.body.article.favoritesCount).to.eql(0);
}
