meta {
  name: Login wrong password
  type: http
  seq: 9
}

post {
  url: {{host}}/api/users/login
  body: json
  auth: none
}

body:json {
  {
    "user": {
      "email": "ea_dup_{{uid}}@test.com",
      "password": "wrongpassword"
    }
  }
}

assert {
  res.status: eq 401
}

script:post-response {
  expect(res.body.errors.credentials[0]).to.eql("invalid");
}
