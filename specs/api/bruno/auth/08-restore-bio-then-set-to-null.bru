meta {
  name: Restore bio then set to null
  type: http
  seq: 8
}

put {
  url: {{host}}/api/user
  body: json
  auth: none
}

headers {
  Authorization: Token {{token}}
}

body:json {
  {
    "user": {
      "bio": "Temporary bio"
    }
  }
}

assert {
  res.status: eq 200
}

script:post-response {
  expect(res.body.user.bio).to.eql("Temporary bio");
}
