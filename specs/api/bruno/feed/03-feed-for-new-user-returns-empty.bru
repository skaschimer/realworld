meta {
  name: Feed for new user returns empty
  type: http
  seq: 3
}

get {
  url: {{host}}/api/articles/feed
  body: none
  auth: none
}

headers {
  Authorization: Token {{main_token}}
}

assert {
  res.status: eq 200
}

script:post-response {
  expect(res.body.articlesCount).to.eql(0);
  expect(res.body.articles.length).to.eql(0);
}
