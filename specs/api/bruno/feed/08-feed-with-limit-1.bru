meta {
  name: Feed with limit=1
  type: http
  seq: 8
}

get {
  url: {{host}}/api/articles/feed?limit=1
  body: none
  auth: none
}

headers {
  Authorization: Token {{main_token}}
}

assert {
  res.status: eq 200
}

script:post-response {
  expect(res.body.articles.length).to.eql(1);
  expect(res.body.articlesCount).to.eql(2);
  expect(res.body.articles[0].author.following).to.eql(true);
}
