meta {
  name: Delete non-existent comment on existing article
  type: http
  seq: 9
}

delete {
  url: {{host}}/api/articles/{{slug}}/comments/99999
  body: none
  auth: none
}

headers {
  Authorization: Token {{token}}
}

assert {
  res.status: eq 404
}

script:post-response {
  expect(res.body.errors.comment[0]).to.eql("not found");
}
